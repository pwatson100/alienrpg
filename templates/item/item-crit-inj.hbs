<section class='tab critical-injury scrollable {{tab.cssClass}}' data-group='primary' data-tab='critical-injury'>
	<div style='border: solid 1px; padding: 5px;width: 629px;'>
		<h2 class='resource-label'>{{localize 'ALIENRPG.Effect'}}</h2>

		{{! Editors must receive enriched text data from getData to properly handle rolls, UUID links, etc. }}
		{{#if editable}}
			<prose-mirror
				style='height: 275px;'
				name='system.attributes.effects'
				data-document-uuid='{{item.uuid}}'
				value='{{system.attributes.effects}}'
				collaborate='true'
				toggled='true'
			>
				{{{enrichedEffect}}}
			</prose-mirror>
		{{else}}
			{{{enrichedEffect}}}
		{{/if}}
	</div>

</section>