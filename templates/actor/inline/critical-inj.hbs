<h2>Hello World</h2>
<ol class='items-list'>
	{{#each critInj as |item iid|}}
		<li class='item critical-inj' data-item-id='{{item.id}}'>
			<a class='item-image crit1a'><img src='{{item.img}}' width='24' height='24' /></a>
			<a class='item-name crit2a'>{{item.name}}</a>
			<a class='item-name crit3a'>{{{striptags item.system.attributes.effects}}}</a>
			<a class='crit4a item-control item-edit1' data-item-id='{{item.id}}' title='{{localize "ALIENRPG.EditItem"}}'><i class='fas fa-edit'></i></a>
		</li>
	{{/each}}
</ol>